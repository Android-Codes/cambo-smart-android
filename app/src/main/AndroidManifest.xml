<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nanita.cambosmart">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cambo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cambo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.HomeActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.PostAdsActivity" />
        <activity android:name=".activities.MyAdsActivity" />
        <activity android:name=".activities.SaveAdsActivity" />
        <activity android:name=".activities.AccountActivity" />
        <activity android:name=".activities.SetMapActivity" />
        <activity android:name=".activities.ChangePasswordActivity" />
        <activity android:name=".activities.LanguageActivity" />
        <activity android:name=".activities.SettingActivity"></activity>
    </application>

</manifest>